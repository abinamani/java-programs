public class pyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 1
		 * 12
		 * 123
		 * 1234
		 * 12345
		 * 1234
		 * 123
		 * 12
		 * 1
		 */
		int n=10;
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print("*");
			}
			
			System.out.println();
		}
		for(int i=1;i<=n;i++) {
			for(int j=n-1;j>=i;j--) {
				System.out.print("*");
			}
			
			System.out.println();
		}
		}
	}
